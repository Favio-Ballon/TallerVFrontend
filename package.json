{
  "name": "fronted",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:unit": "ng test --watch=false",
    "test:unit:watch": "ng test",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:component": "cypress run --component --spec \"cypress/component/**/*.spec.ts\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:component": "cypress open --component",
    "test:jest": "set \"NODE_OPTIONS=--experimental-vm-modules\" && jest --config jest.config.js --runInBand",
    "test:jest:watch": "set \"NODE_OPTIONS=--experimental-vm-modules\" && jest --config jest.config.js --watch",
    "test:jest:ci": "set \"NODE_OPTIONS=--experimental-vm-modules\" && jest --config jest.config.js --ci --runInBand"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.11",
    "@angular/compiler-cli": "^20.3.13",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jest": "^30.0.0",
    "cypress": "^15.7.0",
    "esbuild": "^0.27.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "start-server-and-test": "^1.15.4",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "webpack-dev-server": "4.15.2",
    "@angular-devkit/build-webpack": {
      "webpack-dev-server": "4.15.2"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
